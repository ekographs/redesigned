    "@types/react-dom": "^18.3.0",
    "@types/sortablejs": "^1.15.1",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "nodemon": "^3.0.3",
    "onchange": "^7.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "ts-node": "^10.9.2"
  }
