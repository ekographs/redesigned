    "flat": "^6.0.1",
    "flowbite": "^1.7.0",
    "flowbite-react": "^0.7.0",
    "formik": "^2.4.2",
    "i18next": "^23.4.4",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-resources-to-backend": "^1.1.4",
    "lodash": "^4.17.21",
    "next": "^14.2.3",
    "next-i18next": "^14.0.0",
    "postcss": "^8.4.38",
    "react": "^18",
    "react-dom": "^18",
    "react-i18next": "^13.1.2",
    "react-sortablejs": "^6.1.4",
    "sortablejs": "^1.15.0",
    "tailwindcss": "^3.4.1",
    "typescript": "5.3.3",
